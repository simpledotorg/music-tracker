<!DOCTYPE html>
<html>
  <head>
    <title>MusicTracker</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <h1>MusicTracker</h1>

    <nav>
      <ul>
        <li><%= link_to "Albums", albums_path %></li>
        <li><%= link_to "user", "#" %>
      </ul>
    </nav>

    <%= yield %>
  </body>
</html>
